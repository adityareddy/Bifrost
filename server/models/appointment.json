{
  "name": "Appointment",
  "base": "PersistedModel",
  "properties": {
    "startTime": {
      "type": "date",
      "required": true
    },
    "endTime": {
      "type": "date"
    },
    "patientName": {
      "type": "string"
    },
    "appointmentType": {
      "type": "string",
      "required": true
    },
    "title": {
      "type": "string"
    },
    "allDay": {
      "type": "boolean"
    },
    "status": {
      "type": "string",
      "default": "NS"
    }
  },
  "validations": [],
  "relations": {
    "doctor": {
      "type": "belongsTo",
      "model": "Doctor",
      "foreignKey": "doctorId"
    },
    "patient": {
      "type": "belongsTo",
      "model": "Patient",
      "foreignKey": "patientId"
    }
  },
  "acls": [],
  "methods": []
}
